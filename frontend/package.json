{
  "name": "ip-finder",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.98",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "config": {
    "files": "**/*.{js,jsx,ts,tsx,yml}"
  },
  "scripts": {
    "prettier": "prettier --write $npm_package_config_files",
    "eslint": "eslint --fix --max-warnings 0 --ignore-path .gitignore $npm_package_config_files ",
    "stylelint": "stylelint --fix \"./**/*.css\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "ci:eslint": "eslint --max-warnings 0 --ignore-path .gitignore $npm_package_config_files ",
    "ci:prettier": "prettier --check $npm_package_config_files",
    "ci:stylelint": "stylelint --allow-empty-input \"./**/*.css\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.0"
  }
}